public String reverse(String s){
	String ret = "";
	for( int k=0; k < s.length(); k += 1){
		ret = s.charAt(k) + ret;
	}
}

public String reverse(String s){
	String ret = "";
	int k=0;
	while (k < s.length()){
		ret = s.charAt(k) + ret;
		k + = 1
	}
}

